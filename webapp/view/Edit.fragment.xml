<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core= "sap.ui.core"
    xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form">

        <Dialog  >
		<f:SimpleForm id="objectForm"
			editable="true"
			layout="ResponsiveGridLayout"
			
			labelSpanXL="4"
			labelSpanL="4"
			labelSpanM="4"
			labelSpanS="12"
			adjustLabelSpan="true"
			emptySpanXL="0"
			emptySpanL="0"
			emptySpanM="0"
			emptySpanS="0"
			columnsXL="3"
			columnsL="2"
			columnsM="2"
			singleContainerFullSize="false" >



            <f:content >
                <Label text="CodMesa" design="Bold" />
				<Input value="{textModel>/CodMesa}"  id="CodMesa"     enabled="false"/>

				<Label text="Fecha" design="Bold" />
				<Input  value="{textModel>/Fecha}" id="Fecha"   enabled="false" />

				<Label text="LlamadoTipo" design="Bold" />
                <ComboBox
                    
                    id="LlamadoTipofirst"
                    enabled="true"
                >
					<core:Item text="1"  key="1"/>
                    <core:Item text="2"  key="2"/>
                    
                   

				</ComboBox>

				<Label text="Docente" design="Bold" />
				<Input id="Docente" value="{textModel>/Docente}"  enabled="{textModel>/enabled}"/>

                <Label text="Carrera" design="Bold" />
				<Input  id="Carrera" value="{textModel>/Carrera}" valueLiveUpdate="true"  enabled="{textModel>/enabled}" />

                <Label text="Materia" design="Bold" />
				<Input  id="Materia" value="{textModel>/Materia}" valueLiveUpdate="true"  enabled="{textModel>/enabled}" />

                <Button type="Accept" text="ok" press="EditMesas"/>
                <Button type="Default" press="closeFragment" text="cancel"/>

            </f:content>



		</f:SimpleForm>
	</Dialog>



</core:FragmentDefinition>